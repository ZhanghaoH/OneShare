image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 6px;
  margin: 0 auto;
  display: block;
}

.topBox {
  left: 50%;
  top: 25%;
  margin-top: -200rpx;
  margin-left: -100rpx;
  position: absolute;
  width: 200rpx;
  height: 300rpx;
  overflow: hidden;
}
/*改动文本两边对齐*/
.aboutText {
  display: block;
  margin: 15px;
  overflow: hidden;
  text-align: justify;
  text-indent: 2em;
}

.bottomBox view {
  text-align: center;
  margin-bottom: 5px;
  color: #a0a0a0;
  font-size: 12px;
}

.softHtp navigator {
  margin-top: 300rpx;
  color: orangered;
  margin-bottom: 5px;
}

.bottomBox {
  position: absolute;
  bottom: 50rpx;
  left: 0;
  width: 100%;
}

.txt1 {
  padding-bottom: 30rpx;
}
